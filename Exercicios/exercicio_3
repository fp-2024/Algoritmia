#include <iostream>

using namespace std;

int main()
{
    int n1 = 0;
    int n2 = 0;
    int n3 = 0;

    cout << "Insira n1" << endl;
    cin >> n1;
    cout << "Insira n2" << endl;
    cin >> n2;
    cout << "Insira n3" << endl;
    cin >> n3;

    if (n1 >= n2 && n1 >= n3)
    {
        cout << n1;
    }
    else if (n2 >= n3)
    {
        cout << n2;
    }
    else
    {
        cout << n3;
    }

    return 0;
}
